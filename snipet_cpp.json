			"\t\tif(isPrime[i]) {",
			"\t\t\tfor(int j=2*i; j<=n; j+=i;) {",
			"\t\t\t\tisPrime[i] = false;",
			"\t\t\t}",
			"\t\t\tp.pb(i);",
			"\t\t}",
			"\t}",
			"\treturn p;"
		]
		"description": "エラトステネスのふるい"
